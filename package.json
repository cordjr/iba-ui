{
  "name": "iba-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install &&  nnpm dev",
    "compile:scss": "sass src/assets/scss/iba-ui.scss src/assets/css/iba-ui.css",
    "minify:scss": "sass src/assets/scss/iba-ui.scss src/assets/css/iba-ui.min.css --style compressed",
    "build:scss": "npm run compile:scss && npm run minify:scss"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.4.0",
    "bootstrap": "^5.2.3",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.2",
    "react-dom": "^18.2.0",
    "react-router": "^6.10.0",
    "react-router-dom": "^6.10.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-bootstrap": "^0.32.32",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "sass": "^1.60.0",
    "typescript": "^4.9.3",
    "vite": "^4.2.0"
  }
}
